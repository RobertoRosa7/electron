{
  "name": "primeiro-app",
  "author": {
    "name": "Roberto Rosa",
    "email": "roberto.rosa7@gmail.com"
  },
  "homepage": "https://github.com/RobertoRosa7/electron/tree/main/primeiro-app",
  "license": "MIT",
  "version": "1.0.0",
  "main": "main.js",
  "description": "the first app in electron using angular",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "build:electron": "ng build --base-href ./ && electron .",
    "build:linux": "electron-builder --linux",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "^11.0.4",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@types/electron": "^1.6.10",
    "aws-sdk": "^2.834.0",
    "highcharts": "^9.0.1",
    "ng2-currency-mask": "^9.0.2",
    "python-shell": "^2.0.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@angular/material": "^11.0.4",
    "@angular/material-moment-adapter": "^11.1.1",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "electron": "^10.3.0",
    "electron-builder": "^22.9.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "moment": "^2.29.1",
    "ngx-electron": "^2.2.0",
    "ngx-indexed-db": "^6.1.2",
    "protractor": "~7.0.0",
    "rxjs": "^6.6.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Primeito App"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "build": {
    "appId": "com.primeiroapp.app",
    "productName": "Primeiro App",
    "linux": {
      "target": [
        "deb"
      ],
      "description": "Este Ã© meu primeiro app com electro e angular",
      "category": "Utility"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "target": "tar.gz",
      "category": "public.app-category.developer-tools"
    },
    "directories": {
      "output": "build"
    }
  }
}
